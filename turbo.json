{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "inputs": ["src/a.js"],
      "outputs": ["build/**"],
      "cache": true
    },
    "copy-if-changed": {
      "dependsOn": ["build"],
      "outputs": ["dist/**"],
      "cache": true
    }
  }
}
